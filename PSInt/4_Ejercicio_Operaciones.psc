Algoritmo Operaciones
	Escribir "Ingresa primer numero:";
	Leer num1;
	Escribir "Ingresa segundo numero:";
	Leer num2;
	suma = num1 + num2;
	resta = num1 - num2;
	division = num1 / num2;
	multiplicacion = num1 * num2;
	Escribir "La suma es:", suma;
	Escribir "La multiplicacion es ", multiplicacion;
	Escribir  "El resultado de restar ",num2," a ",num1," es:", resta;
	Escribir "El resultado de dividir ", num1, " entre ", num2, " es:", division;
FinAlgoritmo
