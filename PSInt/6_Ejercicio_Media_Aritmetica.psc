Algoritmo Media_Aritmetica
	Escribir "Coloca el primer numero:";
	Leer num1;
	Escribir "Coloca el segundo numero:";
	Leer num2;
	Escribir "Coloca el tercer numero:";
	Leer num3;
	media <- (num1+num2+num3)/3;
	Escribir "La media aritmetica de los tres numeros proporcionados es: ",media;
FinAlgoritmo
